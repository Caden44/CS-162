from tkinter import *

Main_window = Tk()

my_text = "This is new"

def counter():
    global my_text

    my_label.config(text=my_text)

my_button = Button(Main_window,
                   text="This is new",
                   command=counter)

my_label = Label(Main_window,
                 text="Change me")

my_label.pack()
my_button.pack()

Main_window.mainloop()
